{
  "name": "www",
  "version": "1.0.0",
  "scripts": {
    "start": "hugo server",
    "build": "hugo --minify && npm run build:functions",
    "build:functions": "netlify-lambda build assets/lambda",
    "build:preview": "npm run build -D -F",
    "server": "hugo server",
    "clean": "rimraf public resources functions",
    "clean:deps": "node scripts/clean-deps",
    "copy:deps": "node scripts/copy-deps",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles && npm run -s lint:markdown",
    "lint:scripts": "eslint assets/js config functions",
    "lint:styles": "stylelint \"assets/scss/**/*.{css,sass,scss,sss,less}\"",
    "lint:markdown": "markdownlint-cli2 \"*.md\" \"content/**/*.md\"",
    "lint:markdown-fix": "markdownlint-cli2-fix \"*.md\" \"content/**/*.md\"",
    "test": "npm run -s lint"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.1",
    "autoprefixer": "^9.8",
    "eslint": "^8.12.0",
    "ga-lite": "^2.1.6",
    "hugo-extended": "^0.100.2",
    "markdownlint-cli2": "^0.4.0",
    "netlify-lambda": "^2.0",
    "postcss-cli": "^7.1",
    "purgecss-whitelister": "^2.4",
    "rimraf": "^3.0",
    "shelljs": "^0.8.5",
    "stylelint": "^14.6",
    "stylelint-config-standard-scss": "^3.0"
  },
  "dependencies": {
    "academicons": "^1.8.6",
    "bootstrap": "^5.1.3",
    "bootstrap": "^4.6.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "jquery-countdown": "^2.2.0",
    "lazysizes": "^5.3.2",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.34",
    "node-add-to-calendar": "^1.0.2",
    "photoswipe": "^4.1.3",
    "popper.js": "^1.16.1",
    "prismjs": "^1.27.0",
    "processing-js": "^1.6.6",
    "rrssb": "^1.14.0",
    "slick-carousel": "^1.8.1"
  }
}
